{
  "name": "get-gulp-inst",
  "version": "0.0.1",
  "description": "Get gulp instance from gulpfile",
  "main": "index.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/_mocha --require should --reporter spec",
    "test-cov": "NODE_ENV=test node ./node_modules/.bin/istanbul cover -x ./test/source/**/* ./node_modules/.bin/_mocha -- --require should",
    "test-travis": "NODE_ENV=test node ./node_modules/.bin/istanbul cover -x ./test/source/**/* ./node_modules/.bin/_mocha --report lcovonly -- --require should"
  },
  "author": "LingyuCoder",
  "license": "MIT",
  "devDependencies": {
    "gulp": "~3.9.0",
    "istanbul": "~0.4.0",
    "mocha": "~2.3.3",
    "should": "~7.1.1",
    "should-promised": "~0.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LingyuCoder/get-gulp-inst"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/LingyuCoder/get-gulp-inst/issues"
  },
  "dependencies": {
    "co": "~4.6.0",
    "fs-extra": "~0.26.2"
  }
}
